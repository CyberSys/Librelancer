<Stylesheet>
	<!-- Generic buttons -->
	<ButtonStyle Width="16" Height="16" Name="xbutton">
		<ButtonStyle.Normal>
			<ButtonAppearance.Background>
				<DisplayModel model="xbutton" Tint="text"/>
			</ButtonAppearance.Background>
		</ButtonStyle.Normal>
		<ButtonStyle.Hover>
			<ButtonAppearance.Background>
				<DisplayModel model="xbutton" Tint="text_hover"/>
			</ButtonAppearance.Background>
		</ButtonStyle.Hover>
	</ButtonStyle>
	<!-- Sell button -->
	<ButtonStyle Width="132" Height="26" Name="trader_sell">
		<ButtonStyle.Normal MarginRight="46" TextSize="13" TextColor="text" TextShadow="black" FontFamily="$ButtonText">
			<ButtonAppearance.Background>
				<DisplayModel model="inv_sellarrow.3db" tint="text" />
			</ButtonAppearance.Background>
			<ButtonAppearance.Border>
				<DisplayWireBorder color="text" width="2"/>
			</ButtonAppearance.Border>
		</ButtonStyle.Normal>
		<ButtonStyle.Hover TextColor="text_hover">
			<ButtonAppearance.Background>
				<DisplayModel model="inv_sellarrow.3db" tint="text_hover" />
			</ButtonAppearance.Background>
			<ButtonAppearance.Border>
				<DisplayWireBorder color="text_hover" width="2"/>
			</ButtonAppearance.Border>
		</ButtonStyle.Hover>
	</ButtonStyle>
	
	<!-- Buy button -->
	<ButtonStyle Width="132" Height="26" Name="trader_buy">
